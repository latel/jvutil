{
  "name": "@jv/util",
  "amdName": "jvUtil",
  "version": "2.0.0",
  "description": "jv utils library",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "exports": {
    ".": {
      "import": "./dist/jvutil.mjs",
      "require": "./dist/jvutil.js"
    },
    "./amount": {
      "import": "./amount/dist/amount.mjs",
      "require": "./amount/dist/amount.js"
    },
    "./math": {
      "import": "./math/dist/math.mjs",
      "require": "./math/dist/math.js"
    }
  },
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:amount": "microbundle build --raw --cwd amount",
    "build:math": "microbundle build --raw --cwd math",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "lodash-es": "^4.17.11"
  },
  "devDependencies": {
    "@babel/helper-module-imports": "^7.10.4",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-tester": "^9.2.0",
    "microbundle": "^0.12.3",
    "npm-run-all": "^4.1.5"
  }
}
